# Ask-CSV Configuration

# PostgreSQL Database URL (required)
DATABASE_URL=postgresql://user:password@localhost:5432/askcsv

# OpenAI API Key (required)
OPENAI_API_KEY=sk-your-openai-api-key

# OpenAI Model (optional, default: gpt-4o)
OPENAI_MODEL=gpt-4o

# API Authentication Token (required)
API_TOKEN=your-secure-api-token

# Server Port (optional, default: 5000)
PORT=5000

# Debug Mode (optional, default: false)
FLASK_DEBUG=false
